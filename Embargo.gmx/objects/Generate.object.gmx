with(Node)
{
    draw_set_colour(c_white)
    draw_set_halign(fa_middle)
    draw_set_font(fnt_tiny)

    if global.VIEW = VIEW_TYPE.RESOURCE
    {
        var j = 0
        
        for (var i = 0; i &lt; global.NUM_RESOURCE_TYPES; i++){
            if(resources[i] != 0){
                iii = 0
                for(var ii = resources[i]; ii &gt; 0; ii--){
                    if ii &gt;= 5{
                    draw_sprite(spr_resourceIcons3, i, x+18+(14*iii), y-10+(14*j))
                    ii -= 4
                    }
                    else{
                    draw_sprite(spr_resourceIcons, i, x+18+(14*iii), y-10+(14*j))
                    }
                    
                    iii += 1
                }
                draw_set_halign(fa_left)
                j++
            }
        }
    }
    
    if global.VIEW = VIEW_TYPE.INFLUENCE or (id = other.hover and global.VIEW != VIEW_TYPE.RESOURCE)
    {
        draw_set_halign(fa_right)
        var j = 0
        
        for (var i = 0; i &lt;= global.NUM_PLAYERS; i++){
            if(population[i] &gt;= .1){
                if i != 0 {
                    draw_sprite(spr_factionIcons, i, x+18, y-10+(14*j))
                }
                draw_set_colour(global.FACTION_COLOUR[i])
                draw_text(x+50, y-10+(14*j),string_format(population[i],2,1))
                j++
            } 
        }
    }
}
